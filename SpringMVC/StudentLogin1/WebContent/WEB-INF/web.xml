<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>StudentLogin1</display-name>
  <welcome-file-list>
    <welcome-file>main.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
  <param-name>driver</param-name>
  <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
  <servlet>
    <description></description>
    
    <display-name>StudentLogin1</display-name>
    <servlet-name>StudentLogin1</servlet-name>
    <servlet-class>com.StudentLogin1.StudentLogin1</servlet-class>
    <init-param>
    <param-name>Institute</param-name>
    <param-value>VIT University</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentLogin1</servlet-name>
    <url-pattern>/StuLo1</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StudentView</display-name>
    <servlet-name>StudentView</servlet-name>
    <servlet-class>com.StudentLogin1.StudentView</servlet-class>
    <init-param>
    <param-name>User</param-name>
    <param-value>Student Details </param-value>
    
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentView</servlet-name>
    <url-pattern>/Stuview</url-pattern>
  </servlet-mapping>
</web-app>